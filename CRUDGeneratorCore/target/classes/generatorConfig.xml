<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd"
        >
<generatorConfiguration>
    <!-- 数据库驱动 -->
    <classPathEntry location="E:\jar\mysql-connector-java-5.1.39-bin.jar"/>

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!-- 表前缀 -->
        <tablePrefix text="t_"/>
        <!-- 字段前缀-->
        <fieldPrefix text="f_"/>
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://123.206.183.199/baina" userId="arnold"
                        password="Arnold@666">
        </jdbcConnection>
        <!-- <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver> -->
        <javaControllerGenerator targetPackage="org.arnold.tt.controller"
                                 targetProject="E:\ideaProject\tt\src\main\java">
        </javaControllerGenerator>

        <javaServiceGenerator targetPackage="org.arnold.tt.service"
                              targetProject="E:\ideaProject\tt\src\main\java">
        </javaServiceGenerator>

        <javaModelGenerator targetPackage="org.arnold.tt.model"
                            targetProject="E:\ideaProject\tt\src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>


        <sqlMapGenerator targetPackage="org.arnold.tt.mapper"
                         targetProject="E:\ideaProject\tt\src\main\java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="org.arnold.tt.dao" targetProject="E:\ideaProject\tt\src\main\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--
        <table tableName="stud" domainObjectName="Stud"></table>
        -->
        <table tableName="t_order"
               selectByExampleQueryId="false"
               enableSelectByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
        ></table>
    </context>
</generatorConfiguration>  